  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noEmit": true,
    "target": "ESNext"
  },
  "include": [
    "src/**/*",
    "vite.config.ts"
  ]
}
